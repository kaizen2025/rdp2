{
  "name": "docucortex-ia",
  "version": "3.0.31",
  "description": "DocuCortex IA - Gestionnaire Intelligent avec Intelligence Artificielle",
  "main": "main.js",
  "scripts": {
    "start": "react-scripts start",
    "electron": "electron . --no-sandbox",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://3000 && electron . --no-sandbox\"",
    "dev": "npm run electron-dev",
    "dev-alt": "PORT=3003 react-scripts start",
    "build": "react-scripts build",
    "build:analyze": "npm run build && npx serve -s build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\"",
    "clean": "rimraf build node_modules/.cache",
    "reset": "npm run clean && rm -rf node_modules package-lock.json && npm install",
    "server:start": "node server/server.js",
    "start:auto": "node scripts/start-react.js",
    "test:app": "concurrently \"npm run server:start\" \"npm run start:auto\" --names \"SERVER,REACT\" --prefix-colors \"cyan,magenta\"",
    "test:app:simple": "concurrently \"npm run server:start\" \"npm run start:auto\"",
    "check-ports": "node -e \"const {findAllPorts} = require('./backend/utils/portUtils'); findAllPorts().then(console.log).catch(console.error);\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "axios": "^1.6.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "electron": "^28.2.0",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "file-type": "^18.7.0",
    "framer-motion": "^10.16.4",
    "fs-extra": "^11.1.1",
    "helmet": "^6.0.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "^2.4.1",
    "react-loading-skeleton": "^3.3.1",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "react-window": "^1.8.8",
    "react-virtualized-auto-sizer": "^1.0.20",
    "recharts": "^2.8.0",
    "uuid": "^9.0.1",
    "web-vitals": "^5.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^27.5.1",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "serve": "^14.2.1",
    "supertest": "^6.3.3",
    "wait-on": "^7.2.0",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "proxy": "http://localhost:5000"
}
