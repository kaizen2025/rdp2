{
  "name": "docucortex-ia",
  "version": "3.0.26",
  "description": "DocuCortex IA - Gestionnaire avec Intelligence Artificielle",
  "main": "electron/main.js",
  "homepage": "./",
  "scripts": {
    "start": "node start-react.js",
    "build": "react-scripts build",
    "server:start": "node server/server.js",
    "electron:dev": "electron .",
    "dev": "concurrently \"npm run server:start\" \"npm run start\"",
    "electron:start": "concurrently \"npm run server:start\" \"npm run start\" \"npm run electron:dev\"",
    "build:exe": "npm run build && electron-builder --win portable"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "axios": "^1.7.2",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.4.1",
    "compromise": "^14.14.4",
    "cors": "^2.8.5",
    "electron-is-dev": "2.0.0",
    "electron-log": "^5.2.4",
    "electron-updater": "^6.3.9",
    "express": "^4.21.2",
    "mammoth": "^1.11.0",
    "multer": "^2.0.2",
    "natural": "^8.1.0",
    "node-nlp": "^5.0.0-alpha.5",
    "pdf-parse": "^2.4.5",
    "pizzip": "^3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "tesseract.js": "^5.1.1",
    "web-vitals": "^5.1.0",
    "ws": "^8.18.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/plugin-transform-object-rest-spread": "^7.28.4",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "electron": "^31.0.0",
    "electron-builder": "^25.1.8",
    "react-scripts": "5.0.1",
    "rimraf": "^6.0.1"
  },
  "build": {
    "appId": "com.docucortex.ia",
    "productName": "DocuCortex IA",
    "win": {
      "target": "portable"
    },
    "portable": {
      "artifactName": "DocuCortex-IA-^${version}-portable.exe"
    }
  }
}
