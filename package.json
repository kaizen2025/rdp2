{
  "name": "docucortex-ia",
  "version": "3.0.26",
  "description": "DocuCortex IA - Gestionnaire avec Intelligence Artificielle",
  "author": "DocuCortex Team",
  "main": "electron/main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "start:auto": "node scripts/start-react.js",
    "build": "react-scripts build",
    "server:dev": "nodemon server/server.js",
    "check:deps": "node scripts/check-dependencies.js",
    "rebuild:native": "npm rebuild better-sqlite3",
    "pre-server:start": "npm run check:deps",
    "server:start": "node server/server.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run start:auto\"",
    "electron:dev": "electron .",
    "dev:electron": "concurrently -k \"npm run server:start\" \"npm run start:auto\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "test:app": "concurrently --names \"SERVER,REACT\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run server:start\" \"npm run start:auto\"",
    "build:exe": "npm run build && electron-builder --win portable",
    "build:versioned": "node scripts/build-versioned.js",
    "clean": "rimraf build dist node_modules/.cache .ports.json .env.local .rebuild-done",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:server": "jest tests/server"
  },
  "build": {
    "extends": null,
    "appId": "com.docucortex.ia",
    "productName": "DocuCortex IA",
    "npmRebuild": false,
    "asar": true,
    "files": [
      "build/**/*",
      "electron/**/*",
      "server/**/*",
      "backend/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "extraResources": [
      {
        "from": "config/",
        "to": "config"
      }
    ],
    "win": {
      "target": "portable",
      "icon": "assets/icon.ico"
    },
    "portable": {
      "artifactName": "DocuCortex-IA-${version}-portable.exe"
    },
    "publish": {
      "provider": "generic",
      "url": "http://192.168.1.232/update/"
    }
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.15.15",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.15",
    "@mui/x-date-pickers": "^6.19.9",
    "@mui/x-tree-view": "^8.16.0",
    "axios": "^1.7.2",
    "better-sqlite3": "^12.4.1",
    "brain.js": "^2.0.0-beta.24",
    "canvg": "^4.0.3",
    "compromise": "^14.14.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dompurify": "^3.3.0",
    "electron-is-dev": "2.0.0",
    "electron-log": "^5.2.4",
    "electron-updater": "^6.3.9",
    "emoji-picker-react": "^4.15.0",
    "express": "^4.19.2",
    "express-validator": "^7.0.1",
    "framer-motion": "^11.0.3",
    "html2canvas": "^1.4.1",
    "iconv-lite": "^0.6.3",
    "jspdf": "^2.5.1",
    "lru-cache": "^11.0.0",
    "mammoth": "^1.6.0",
    "multer": "^2.0.2",
    "natural": "^6.10.0",
    "node-nlp": "^4.27.0",
    "path-browserify": "^1.0.1",
    "pdf-parse": "^1.1.1",
    "pizzip": "^3.1.7",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.5.0",
    "react-dropzone": "^14.2.3",
    "react-grid-layout": "^1.4.4",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.23.1",
    "react-toastify": "^10.0.4",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "recharts": "^2.12.0",
    "tesseract.js": "^5.1.1",
    "web-vitals": "^5.1.0",
    "ws": "^8.18.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.24.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "node-windows": "^1.0.0-beta.8",
    "nodemon": "^3.1.3",
    "react-scripts": "5.0.1",
    "rimraf": "^6.0.1",
    "supertest": "^6.3.3",
    "wait-on": "^7.2.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
