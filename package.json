{
  "name": "rds-viewer",
  "version": "3.0.26",
  "description": "RDS Viewer - Gestionnaire de sessions RDS avec Intelligence Artificielle (DocuCortex)",
  "author": "Anecoop <support@anecoop.com>",
  "main": "electron/main.js",
  "homepage": "./",
  "scripts": {
    "start": "craco start",
    "build": "cross-env NODE_ENV=production GENERATE_SOURCEMAP=false craco build",
    "server:start": "node server/server.js",
    "electron:dev": "node scripts/wait-for-backend.js && electron .",
    "dev": "concurrently \"npm run server:start\" \"npm run start\"",
    "electron:start": "concurrently \"npm run server:start\" \"npm run start\" \"npm run electron:dev\"",
    "rebuild:native": "node scripts/rebuild-native.js",
    "build:exe": "npm run build && npm run rebuild:native && electron-builder --win portable --config electron-builder-portable.json",
    "build:portable": "npm run build && npm run rebuild:native && electron-builder --win portable --x64 --config electron-builder-portable.json",
    "build:optimized": "npm run build && npm run rebuild:native && electron-builder --config electron-builder-optimized.json --win portable --x64",
    "build:installer": "npm run build && npm run rebuild:native && electron-builder --win nsis --x64 --config electron-builder.json",
    "build:all": "npm run build && npm run rebuild:native && electron-builder --win --x64 --config electron-builder.json",
    "build:linux": "npm run build && npm run rebuild:native && electron-builder --linux --config electron-builder.json",
    "build:mac": "npm run build && npm run rebuild:native && electron-builder --mac --config electron-builder.json",
    "dist": "npm run build:all"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@google/generative-ai": "^0.24.1",
    "@mui/icons-material": "^5.15.15",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.15",
    "@mui/x-date-pickers": "^6.20.2",
    "@tanstack/react-query": "^5.56.2",
    "axios": "^1.7.2",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.4.1",
    "chokidar": "^3.6.0",
    "compromise": "^14.14.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "electron-is-dev": "2.0.0",
    "electron-log": "^5.2.4",
    "electron-updater": "^6.3.9",
    "emoji-picker-react": "^4.15.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "html2canvas": "^1.4.1",
    "http-proxy-middleware": "^2.0.6",
    "iconv-lite": "^0.6.3",
