    "natural": "^8.1.0",
    "node-fetch": "^3.3.2",
    "node-nlp": "^5.0.0-alpha.5",
    "nodemailer": "^7.0.10",
    "pdf-parse": "^2.4.5",
    "pdfjs-dist": "^5.4.394",
    "pizzip": "^3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.5.0",
    "react-dropzone": "^14.3.8",
    "react-lazy-load-image-component": "^1.6.2",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.23.1",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^2.2.2",
    "tesseract.js": "^5.1.1",
    "ws": "^8.18.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
