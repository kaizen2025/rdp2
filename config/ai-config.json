{
  "aiProvider": "openrouter",
  "providers": {
    "openrouter": {
      "enabled": true,
      "priority": 1,
      "apiKey": "STORED_IN_ENV_FILE",
      "baseUrl": "https://openrouter.ai/api/v1",
      "model": "openrouter/polaris-alpha",
      "timeout": 120000,
      "temperature": 0.7,
      "max_tokens": 4096
    }
  },
  "fallback": {
    "enabled": true,
    "autoSwitch": true,
    "retryAttempts": 3
  },
  "ocr": {
    "enabled": true,
    "languages": ["fr", "en", "es", "de", "it", "pt", "ru", "ja", "ko", "zh-cn", "zh-tw"],
    "confidence_threshold": 0.6,
    "gpu": false
  },
  "chat": {
    "max_context": 10,
    "auto_save": true,
    "theme": "light"
  },
  "network": {
    "timeout": 30,
    "retries": 3
  },
  "ui": {
    "language": "fr",
    "theme": "light",
    "autoSave": true
  }
}
