{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "react-app"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-unused-vars": "warn",
    "react/no-unescaped-entities": "warn",
    "react/display-name": "warn",
    "no-unreachable": "warn",
    "no-dupe-class-members": "warn"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
